
@{
    ViewBag.Title = "Configuration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Configuracion de excel</h2>
<div class="card">
    <div class="card-body">
        <div class="d-flex justify-content-center align-content-center gap-5 flex-column">
            <div class="col row justify-content-center align-items-center">
                <img class="col-4" src="data:image/jpeg;base64," />
            </div>
            @using (Html.BeginForm("Configuration", "Excel", FormMethod.Post, new { @class = "col-12 row g-3 justify-content-center align-items-center flex-column", @enctype = "multipart/form-data", @id="FormExcel" }))
            {
                <div class="col-3">
                    <span class="fs-4">Sube tus archivos de Excel</span>
                </div>
                <div class="col-3">
                    <label for="Excel"></label>
                    <input name="Excel" id="Exce" type="file" class="form-control col" />
                </div>
                <div class="col-3">
                    <div class="row">
                        <button class="col btn btn-success" data-bs-toggle="modal" data-bs-target="#Mensaje">
                            Aceptar
                        </button>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<div class="modal fade" data-bs-backdrop="static" id="Mensaje">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body" id="modalBody">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>
